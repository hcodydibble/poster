#!/Users/hcodydibble/Documents/SCHOOL/codefellows/401/poster/ENV/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'poster','console_scripts','post'
__requires__ = 'poster'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('poster', 'console_scripts', 'post')()
    )
